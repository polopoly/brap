    <body>
        <release version="0.9" date="2009-08-03" description="First stable release">
            <action dev="Edvin Syse" type="add">
                Initial release.
            </action>
        </release>
        <release version="0.9.1" date="2009-08-22" description="">
            <action dev="Edvin Syse" type="add">
                Automatically sync changes to method arguments from the server to the client
            </action>
            <action dev="Edvin Syse" type="fix">
                SingleUsernamePasswordAuthenticator throws AuthenticationFailedException even with correct credentials
            </action>
        </release>
        <release version="0.9.2" date="2009-09-12" description="">
            <action dev="Edvin Syse" type="add">
                Support for generics on the client
            </action>
        </release>
        <release version="0.9.3" date="2010-07-06" description="">
